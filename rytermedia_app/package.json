{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.25.0",
  "scripts": {
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --fix --ext .vue,.ts,.js",
    "build": "nuxt build",
    "comment-about-dev-no-fork": "sometimes it seems needed, other times it seems to work with it...more info https://github.com/nuxt/cli/issues/181",
    "dev": "nuxt dev --no-fork",
    "generate": "nuxt generate",
    "generate-offline": "NUXT_APP_BASE_URL=./ NUXT_OFFLINE_SINGLE_HTML=1 nuxt generate && ",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && pnpm exec playwright-core install --only-shell chromium-headless-shell",
    "test-unit": "vitest --exclude=\"tests/e2e/**\" --exclude=\"tests/compiled/**\" --run --coverage",
    "test-unit:watch": "vitest --exclude=\"tests/e2e/**\" --exclude=\"tests/compiled/**\"",
    "test-compiled": "vitest --exclude=\"tests/e2e/**\" --exclude=\"tests/unit/**\" --test-timeout=15000 --run",
    "test-e2e": "docker compose --file=../docker-compose.yaml build && dotenv -v USE_DOCKER_COMPOSE_FOR_VITEST_E2E=1 -- vitest --exclude=\"tests/unit/**\" --exclude=\"tests/compiled/**\" --test-timeout=15000 --run",
    "test-compiled:watch": "vitest --exclude=\"tests/e2e/**\" --exclude=\"tests/unit/**\" --test-timeout=15000"
  },
  "dependencies": {
    "@fontsource/instrument-serif": "^5.2.8",
    "@fontsource/public-sans": "^5.2.7",
    "@iconify-json/lucide": "^1.2.71",
    "@iconify-json/simple-icons": "^1.2.60",
    "@iconify/vue": "^5.0.0",
    "@nuxt/content": "^3.9.0",
    "@nuxt/fonts": "^0.12.1",
    "@nuxt/icon": "^2.1.0",
    "@nuxt/image": "^2.0.0",
    "@nuxt/ui": "^4.2.1",
    "@nuxtjs/color-mode": "^3.5.2",
    "@vueuse/nuxt": "^13.9.0",
    "better-sqlite3": "^12.4.6",
    "motion-v": "^1.7.3",
    "nuxt": "^4.2.2",
    "nuxt-og-image": "^5.1.12",
    "vue": "^3.5.25",
    "vue-router": "^4.6.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@iconify-json/mdi": "^1.2.3",
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/schema": "^4.2.2",
    "@nuxt/test-utils": "3.19.1",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@playwright/test": "^1.57.0",
    "@types/node": "^25.0.0",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vue/devtools-api": "^8.0.0",
    "@vue/test-utils": "^2.4.6",
    "autoprefixer": "^10.4.21",
    "debug": "^4.4.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "~9.38.0",
    "eslint-plugin-vitest": "^0.5.4",
    "happy-dom": "^20.0.2",
    "nuxt-single-html": "^0.2.0",
    "parse5": "^8.0.0",
    "playwright": "^1.57.0",
    "playwright-core": "^1.57.0",
    "postcss": "^8.5.3",
    "rehype-raw": "^7.0.0",
    "remark-emoji": "^5.0.2",
    "remark-gfm": "^4.0.1",
    "remark-mdc": "^3.9.0",
    "remark-rehype": "^11.1.2",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.9.3",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.1.3",
    "vue-tsc": "^3.1.2"
  }
}
